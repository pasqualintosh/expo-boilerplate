{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": [
      "expo-router/ts-declarations"
    ],
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "@*": [
        "./*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
}
